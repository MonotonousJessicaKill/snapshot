<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="jielin.snapshot.domain.IcmWadeProdEntity" table="icm_wade_prod" schema="wade">
        <id name="id">
            <column name="ID" sql-type="int(15)"/>
        </id>
        <property name="severity">
            <column name="Severity" sql-type="varchar(5)" length="5"/>
        </property>
        <property name="status">
            <column name="Status" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="title">
            <column name="Title" sql-type="varchar(2000)" length="2000"/>
        </property>
        <property name="role">
            <column name="Role" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="owner">
            <column name="Owner" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="environment">
            <column name="Environment" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
        <property name="incidentType">
            <column name="IncidentType" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="effort">
            <column name="Effort" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="createDate">
            <column name="CreateDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="acknowledgeDate">
            <column name="AcknowledgeDate" sql-type="varchar(2000)" length="2000" not-null="true"/>
        </property>
        <property name="modifiedDate">
            <column name="ModifiedDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="spendTime">
            <column name="SpendTime" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
        <property name="updatetime">
            <column name="Updatetime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="inQuery">
            <column name="InQuery" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="reCreateDate">
            <column name="ReCreateDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="tta">
            <column name="TTA" sql-type="varchar(2000)" length="2000"/>
        </property>
    </class>
</hibernate-mapping>